func chain_one(f64 x, f64 y, f64 z) => f64 {
    return x + y - z;
}

func chain_two(f64 x, f64 y, f64 z) => f64 {
    return x * y / z;
}

func chain_three(f64 x, f64 y, f64 z) => f64 {
    return (x + y) * z;
}

func six_seven() => f64 {
    let a = chain_one(5.0, 4.5, 2.5);     // 7
    let b = chain_two(2.0, 3.5, a);     // 1
    let c = chain_three(b, 1.0, 0.35);  // 0.7

    return a - b + c; // 6.7
}

return six_seven();